<%-include("header")-%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Title</label>
            <input type="text" name="blog" autocomplete="off" class="form-control" id="exampleInputEmail1"
                aria-describedby="emailHelp">

        </div>
        <div class="mb-3">
            <label style="margin-top: 10px;" for="exampleInputPassword1" class="form-label">Blog</label>
            <textarea name="blogBody" id="" cols="30" rows="10" class="form-control"
                id="exampleInputPassword1"></textarea>
        </div>

        <button style="margin-top: 10px;" type="submit" class="btn btn-primary">Publish</button>
    </form>

    <%-include("footer")-%>